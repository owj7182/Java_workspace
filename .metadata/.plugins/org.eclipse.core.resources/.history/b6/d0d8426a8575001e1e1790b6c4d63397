package compony;

public class Compony {
	private double salary;
	private double annulIncome;
	private double income;
	private double bonus;
	private double afterTaxBonus;
	
	public Compony() {
		super();
	}

	public Compony(double salary) {
		super();
		this.salary = salary;
	}

	double getSalary() {
		return salary;
	}

	double getAnnulIncome() {
		return annulIncome;
	}

	double getIncome() {
		return income;
	}

	double getBonus() {
		return bonus;
	}

	double getAfterTaxBonus() {
		return afterTaxBonus;
	}

	@Override
	public String toString() {
		return "Compony [salary=" + salary + ", annulIncome=" + annulIncome + ", income=" + income + ", bonus=" + bonus
				+ ", afterTaxBonus=" + afterTaxBonus + "]";
	}

	
	
	
}
